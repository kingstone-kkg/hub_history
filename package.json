{
    "name": "hubl",
    "displayName": "HubL Language Extension",
    "description": "Snippets for HubL",
    "version": "0.0.1",
    "publisher": "WilliamSpiro",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/hubl_filters.json"
            },
            {
                "language": "html",
                "path": "./snippets/hubl_functions.json"
            },
            {
                "language": "html",
                "path": "./snippets/hubl_tags.json"
            },
            {
                "language": "html",
                "path": "./snippets/hubl_extras.json"
            }
        ],
        "languages": [{
            "id": "html",
            "extensions": [ ".html" ],
            "configuration": "./langconfig/language-configuration.json"
        }]
    }
}